<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:left="2dp"
        android:right="2dp"
        android:top="5dp"
        android:bottom="4dp"
        >
        <shape android:shape="rectangle">
            <solid android:color="@color/kuning"/>
            <corners android:radius="15dp" />
        </shape>
    </item>

    <item
        android:left="0dp"
        android:right="0dp"
        android:top="0dp"
        android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/kuning"/>
            <corners android:radius="15dp" />
            <stroke
                android:width="1dp"
                android:color="@color/kuning" />
        </shape>
    </item>
</layer-list>